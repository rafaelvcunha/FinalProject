                  <TableColumn fx:id="clCpfCliente" prefWidth="68.0" text="CPF Cliente" />
                  <TableColumn fx:id="clStatusContrato" prefWidth="72.0" text="Status Contrato" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" prefHeight="62.0" prefWidth="749.0" spacing="15.0">
