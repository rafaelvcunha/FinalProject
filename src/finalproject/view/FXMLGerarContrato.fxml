                  <DatePicker fx:id="dpDataServicoInicio" promptText="Data Início Serviço" />
                  <DatePicker fx:id="dpDataServicoFim" promptText="Data Fim Serviço" />
                  <TextField fx:id="txtFotoDoLocal" prefHeight="25.0" prefWidth="306.0" promptText="Caminho Foto Do Local" />
               </children>
