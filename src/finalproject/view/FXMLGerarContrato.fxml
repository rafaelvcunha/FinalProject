                  <TextField fx:id="txtCorTinta" promptText="Cor Tinta" />
                  <TextField fx:id="txtRevestimento" promptText="Revestimento" />
                  <TextField fx:id="txtTipoPapelParede" prefHeight="25.0" prefWidth="182.0" promptText="Tipo Papel Parede" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="62.0" prefWidth="749.0" spacing="15.0">
