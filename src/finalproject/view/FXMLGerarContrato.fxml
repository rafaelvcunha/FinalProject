               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <TableView fx:id="tblContratos" prefHeight="266.0" prefWidth="749.0">
              <columns>
                  <TableColumn fx:id="clCodContrato" prefWidth="54.0" text="Cod Contrato" />
                <TableColumn fx:id="clCampoMetragem" prefWidth="53.0" text="Metragem" />
                <TableColumn fx:id="clCorTinta" minWidth="3.0" prefWidth="53.0" text="Cor Tinta" />
